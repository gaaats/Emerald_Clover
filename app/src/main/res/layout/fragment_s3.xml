        app:layout_constraintStart_toStartOf="@id/guideline_vert_85"
        app:layout_constraintTop_toTopOf="@id/frhuhghuthgthutg" />



    <LinearLayout
        android:id="@+id/lin_lay_game"
        android:layout_width="0dp"
        android:layout_height="300dp"
        android:background="@drawable/card_v_back"
        android:orientation="horizontal"
        android:padding="16dp"
        android:weightSum="9"
        app:layout_constraintBottom_toBottomOf="@id/tngtgtgtjigtij"
        app:layout_constraintEnd_toEndOf="@id/fgkktogkotgk"
        app:layout_constraintStart_toStartOf="@id/rfrmgttgjgtgttggt"
        app:layout_constraintTop_toTopOf="@id/efrkigtjjijtyhjiy">


        <LinearLayout
            android:id="@+id/lin_left"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:orientation="vertical">

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/rec_v_left"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                tools:listitem="@layout/elelelelle" />

        </LinearLayout>

        <androidx.cardview.widget.CardView
            android:layout_width="3dp"
            android:layout_height="match_parent"
            android:layout_marginTop="40dp"
            android:layout_marginBottom="40dp"
            android:background="#3EED5C"
            app:cardCornerRadius="16dp">

            <View
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#3EED5C" />

        </androidx.cardview.widget.CardView>


        <LinearLayout
            android:id="@+id/lin_center"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:orientation="vertical">

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/rec_v_center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                tools:listitem="@layout/elelelelle" />

        </LinearLayout>

        <androidx.cardview.widget.CardView
            android:id="@+id/card_right"
            android:layout_width="3dp"
            android:layout_height="match_parent"
            android:layout_marginTop="40dp"
            android:layout_marginBottom="40dp"
            android:background="#3EED5C"
            app:cardCornerRadius="16dp">

            <View
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#3EED5C" />

        </androidx.cardview.widget.CardView>


        <LinearLayout
            android:id="@+id/lin_right"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:orientation="vertical">

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/rec_v_right"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                tools:listitem="@layout/elelelelle" />

        </LinearLayout>
    </LinearLayout>

    <com.google.android.material.button.MaterialButton
