        android:layout_height="30dp"
        android:scaleType="centerInside"
        android:src="@drawable/f6"
        app:layout_constraintTop_toBottomOf="@id/lin_lay_game"
        app:layout_constraintEnd_toEndOf="@+id/vmgitigtihgtuihiutghgt"
